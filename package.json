{
	"name": "@c3d.gg/mwi-types",
	"version": "0.2.5",
	"author": "C3D",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/c3d-gg/mwi-types.git"
	},
	"main": "./dist/index.js",
	"module": "index.ts",
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "^4.7.0",
		"@types/bun": "^1.2.21",
		"@types/jest": "^30.0.0",
		"@types/prettier": "^3.0.0",
		"c8": "^10.1.3",
		"fx": "^39.0.4",
		"jq": "^1.7.2",
		"prettier": "^3.6.2",
		"ts-morph": "^26.0.0"
	},
	"peerDependencies": {
		"typescript": ">=5.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"bugs": {
		"url": "https://github.com/c3d-gg/mwi-types/issues"
	},
	"description": "TypeScript type definitions and schemas for Milky Way Idle game data",
	"engines": {
		"node": ">=16.0.0"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"homepage": "https://github.com/c3d-gg/mwi-types#readme",
	"keywords": [
		"milky-way-idle",
		"mwi",
		"typescript",
		"types",
		"schemas",
		"zod",
		"typebox",
		"game-data",
		"type-generation",
		"c3d"
	],
	"license": "MIT",
	"scripts": {
		"generate": "bun run src/generation/index.ts",
		"generate:watch": "bun run --watch src/generation/index.ts",
		"typecheck": "bun tsc --noEmit",
		"format": "prettier --write ./src",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"dev": "bun scripts/mwi-dev.ts",
		"build:tsc": "tsc -p tsconfig.build.json",
		"clean": "rm -rf src/generated",
		"clean:dist": "rm -rf dist",
		"prebuild": "bun run generate",
		"prepublishOnly": "bun run build"
	},
	"sideEffects": false,
	"type": "module",
	"types": "./dist/index.d.ts"
}
