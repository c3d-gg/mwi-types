{
  "name": "@c3d.gg/mwi-types",
  "module": "index.ts",
  "type": "module",
  "version": "0.2.1",
  "description": "TypeScript type definitions and schemas for Milky Way Idle game data",
  "author": "C3D",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c3d-gg/mwi-types.git"
  },
  "homepage": "https://github.com/c3d-gg/mwi-types#readme",
  "bugs": {
    "url": "https://github.com/c3d-gg/mwi-types/issues"
  },
  "keywords": [
    "milky-way-idle",
    "mwi",
    "typescript",
    "types",
    "schemas",
    "zod",
    "typebox",
    "game-data",
    "type-generation",
    "c3d"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "generate": "bun run src/generation/index.ts",
    "generate:watch": "bun run --watch src/generation/index.ts",
    "typecheck": "bun tsc --noEmit",
    "format": "prettier --write ./src",
    "build": "bun run clean:dist && bun run build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rm -rf src/generated",
    "clean:dist": "rm -rf dist",
    "prebuild": "bun run generate",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@types/bun": "latest",
    "@types/prettier": "^3.0.0",
    "chalk": "^5.5.0",
    "prettier": "^3.6.2",
    "ts-morph": "^26.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {}
}
